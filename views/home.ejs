<%- include("partials/header") -%>
<h1>Hello</h1>
<p><%= home %></p>
<div class="home-posts">
<% allPosts.forEach(function(post){ %>
     <div>
        <h2><%= post.title %></h2>
        <p>
            <%= post.content.substring(0,70) + "..." %>
            <a href= "/posts/<%= post._id %>" >Read More</a>
        </p>
    </div>
<% }) %>
</div>
<%- include("partials/footer") -%>